Router _$SupplierControllerRouter(SupplierController service) {
  final router = Router();
  router.add(
    'GET',
    r'/',
    service.find,
  );
  router.add(
    'POST',
    r'/',
    service.registerSupplier,
  );
  router.add(
    'GET',
    r'/user',
    service.userExistesByEmail,
  );
  router.add(
    'GET',
    r'/<id>',
    service.findById,
  );
  return router;
}

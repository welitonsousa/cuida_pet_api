Router _$AuthControllerRouter(AuthController service) {
  final router = Router();
  router.add(
    'POST',
    r'/register',
    service.find,
  );
  router.add(
    'PUT',
    r'/refresh',
    service.refresh,
  );
  router.add(
    'POST',
    r'/sign',
    service.sign,
  );
  return router;
}
